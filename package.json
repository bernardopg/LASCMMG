{
  "name": "lascmmg",
  "version": "1.1.0",
  "description": "Sistema de gerenciamento de torneios da LASCMMG",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "lint": "eslint . --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "frontend:dev": "cd frontend-react && npm run dev",
    "frontend:build": "cd frontend-react && npm run build",
    "frontend:preview": "cd frontend-react && npm run preview",
    "frontend:lint": "cd frontend-react && npm run lint",
    "frontend:lint:fix": "cd frontend-react && npm run lint:fix",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "generate-keys": "node scripts/generate-keys.js",
    "generate-keys:backup": "node scripts/generate-keys.js backup",
    "setup:env": "node scripts/generate-keys.js all",
    "setup:db": "node backend/setup-db.js",
    "health-check": "node scripts/health-check.js"
  },
  "keywords": [
    "torneio",
    "gerenciamento",
    "competição",
    "chaveamento"
  ],
  "author": "LASCMMG",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.10.0",
    "commander": "^14.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "proper-lockfile": "^4.1.2",
    "redis": "^5.1.0",
    "socket.io": "^4.8.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/ui": "^3.1.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}
